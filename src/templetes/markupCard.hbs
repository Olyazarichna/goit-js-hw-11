{{#each this}}
<li class="card-list">
<div class="photo-card">
  <div class="gallery">
    <a href="{{webformatURL}}"><img src="{{largeImageURL}}" alt="{{tags}}" title="" loading="lazy" /></a>
  <div class="info">
    <p class="info-item">
      <b>Likes: {{likes}}</b>
    </p>
    <p class="info-item">
      <b>Views: {{views}}</b>
    </p>
    <p class="info-item">
      <b>Comments: {{comments}}</b>
    </p>
    <p class="info-item">
      <b>Downloads: {{downloads}}</b>
    </p>
  </div>
</div>
</li>
{{/each}}